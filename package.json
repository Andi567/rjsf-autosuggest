{
  "name": "@unrest/core",
  "description": "Kitchen sink repo for personal react projects",
  "version": "0.0.3",
  "main": "dist.js",
  "directories": {
    "lib": "dist"
  },
  "repository": "https://github.com/chriscauley/unrest-core.git",
  "author": "chriscauley",
  "license": "MIT",
  "scripts": {
    "build": "rm -rf dist && yarn babel src -d dist; yarn build/docs",
    "build/docs": "rm -rf docs && yarn parcel build demo/index.html -d docs --public-url /rjsf-autosuggest/",
    "lint": "yarn eslint src/ demo",
    "test": "yarn jest",
    "develop": "yarn parcel demo/index.html -d .dist"
  },
  "peerDependencies": {
    "react": ">=15"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@testing-library/react": "^10.0.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "classnames": "^2.2.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.19.0",
    "jest": "^25.3.0",
    "parcel": "^1.12.4",
    "pokemon": "^2.0.2",
    "prettier": "^2.0.4",
    "react-dom": "^16.13.1",
    "react-router-dom": "^5.2.0",
    "react-tabs": "^3.1.0",
    "react-test-renderer": "^16.13.1"
  },
  "dependencies": {
    "react-autosuggest": "^10.0.2"
  }
